
---
layout: null
permalink: /search.json
---
[
{% assign all = site.protocols | concat: site.materials | concat: site.tools | concat: site.atlas | concat: site.curiosities %}
{% for p in all %}
  {
    "title": {{ p.title | jsonify }},
    "url": {{ p.url | relative_url | jsonify }},
    "section": {{ p.collection | jsonify }},
    "tags": {{ p.tags | join: " " | jsonify }},
    "excerpt": {{ p.excerpt | strip_html | jsonify }}
  }{% unless forloop.last %},{% endunless %}
{% endfor %}
]
